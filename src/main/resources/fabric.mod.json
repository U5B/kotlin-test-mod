{
  "schemaVersion": 1,
  "id": "${base_name}",
  "version": "${mod_version}",
  "name": "base.kt",
  "description": "Port of U5B_'s jsmacro scripts!",
  "authors": [
    "U5B_"
  ],
  "contact": {
    "homepage": "https://discord.usbwire.net",
    "sources": "https://github.com/U5B/kotlin-test-mod"
  },
  "license": "GPL",
  "icon": "assets/usb/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "net.usbwire.base.BaseMod"
      }
    ],
    "modmenu": [
      {
        "adapter": "kotlin",
        "value": "net.usbwire.base.config.ModMenu"
      }
    ]
  },
  "mixins": [
    "base.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=17",
    "fabric": ">=${fabric_api_version}",
    "fabric-language-kotlin": ">=1.9.4"
  },
  "suggests": {
    "modmenu": "*"
  }
}