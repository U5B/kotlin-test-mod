{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "U5B_"
  ],
  "contact": {
    "homepage": "https://discord.usbwire.net",
    "issues": "https://github.com/U5B/usbplus/issues",
    "sources": "https://github.com/U5B/usbplus"
  },
  "license": "GPL",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "value": "${maven_group}.USBPlus"
      }
    ],
    "modmenu": [
      {
        "value": "${maven_group}.config.ModMenu"
      }
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "=${minecraft_version}",
    "java": ">=17",
    "fabric-api": ">=${fabric_api_version}+${minecraft_version}",
    "fabric-language-kotlin": "*",
    "cloud": "*"
  },
  "suggests": {
    "modmenu": "*"
  }
}